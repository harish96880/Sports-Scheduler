    <section>
      <h1 class="m-5" style="font-family: sans-serif; color: rgba(255, 166, 0, 0.819); font-size: 3.5vw;">Available <span style="color:rgba(255, 255, 255, 0.772)">Sports</span></h1>
      <div class="grid">
        <% sportsItems.forEach(function(user) { %>
        <div class="card">
          
          <h1 style="color: transparent; font-family: sans-serif;"><%= user.id %></h1>
            <h1 style="color: rgba(255, 255, 255, 0.819); font-family: sans-serif;" class="getSportId" id="getNameOfSport" onclick="window.location.href='/SportDetail'"><%= user.Sports_Name %></h1>
            <div class="row">
              <div class="col-lg-12 my-5 sportsButtonsDiv">
                <a class="sportsButtons btn btn-danger deleteSport" href = '' style="color: transparent; padding-left: 22px;"><svg xmlns="http://www.w3.org/2000/svg" width="26" height="26" fill="currentColor" class="bi bi-trash3-fill" viewBox="0 0 16 16">
                  <path d="M11 1.5v1h3.5a.5.5 0 0 1 0 1h-.538l-.853 10.66A2 2 0 0 1 11.115 16h-6.23a2 2 0 0 1-1.994-1.84L2.038 3.5H1.5a.5.5 0 0 1 0-1H5v-1A1.5 1.5 0 0 1 6.5 0h3A1.5 1.5 0 0 1 11 1.5Zm-5 0v1h4v-1a.5.5 0 0 0-.5-.5h-3a.5.5 0 0 0-.5.5ZM4.5 5.029l.5 8.5a.5.5 0 1 0 .998-.06l-.5-8.5a.5.5 0 1 0-.998.06Zm6.53-.528a.5.5 0 0 0-.528.47l-.5 8.5a.5.5 0 0 0 .998.058l.5-8.5a.5.5 0 0 0-.47-.528ZM8 4.5a.5.5 0 0 0-.5.5v8.5a.5.5 0 0 0 1 0V5a.5.5 0 0 0-.5-.5Z"/>
                </svg><%= user.id %></a>
              </div>
            </div>
        </div>
        
        <% }); %>
        <script>
          const redirectAnchors = document.querySelectorAll("h1.getSportId");
          redirectAnchors.forEach((anchor) => {
            anchor.addEventListener("click", function (event) {
              event.preventDefault();
              const text = this.innerText;
              const url = "/Sports/" + encodeURIComponent(text);
              this.setAttribute("href", url);
              window.location.href = url;
             });
          });
        </script>
        <script>
          const deleteanchors = document.querySelectorAll("a.deleteSport");
          deleteanchors.forEach((anchor) => {
            anchor.addEventListener("click", function (event) {
              event.preventDefault();
              const text = this.innerText;
              window.alert(text)
              const url = "/delete/" + encodeURIComponent(text);
              this.setAttribute("href", url);
              window.location.href = url; 
      });
    });
        </script>
        <script>
            const anchors = document.querySelectorAll("a.link");
            anchors.forEach((anchor) => {
              anchor.addEventListener("click", function (event) {
                event.preventDefault(); 
                const text = this.innerText;
                const url = "/Sports/" + encodeURIComponent(text);
                this.setAttribute("href", url);
                window.location.href = url; 
              });
            });
          </script>
      </div>
      
    </section>
  </body>
</html>
